<?xml version="1.0" encoding="utf-8"?>


<odoo>
    <data>

    <record id="view_demo_data_tree" model="ir.ui.view">
      <field name="name">view.demo.data.tree</field>
      <field name="model">x_rgpd_medidas.data</field>
      <field name="arch" type="xml">
        <tree string="tree">

          <field name="name"/>
          <field name="date_from"/>

        </tree>
      </field>

    </record>

    <record id="view_demo_data_form" model="ir.ui.view">
      <field name="name">view.demo.data.form</field>
      <field name="model">x_rgpd_medidas.data</field>
      <field name="arch" type="xml">
        <form name="demo data form">
          <sheet>
            <div class="oe_tittle">
              <label for="name" class="oe_edit_only"/>
              <h1>
                <field name="name" placeholder="request"/>
              </h1>
            </div>
            <group>
              <field name="date_from"/>
            </group>
          </sheet>
        </form>
      </field>

    </record>

    <record id="action_demo_data_views" model="ir.actions.act_window">
      <field name="name">demo request</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">demo.data</field>
      <field name="view_mode">form</field>
      <field name="help" type="html">
       <p class="oe_view_nocontent_create"></p>
        <p>Click to --- </p>
      </field>

    </record>


    <menuitem id="menu_demo_data_root" name="DemoDataLib" sequence="10"/>
    <menuitem id="menu_demo_request_categ" name="DemoData" parent="menu_demo_data_root" sequence="1"/>
    <menuitem id="menu_demo_request" name="DemoData" parent="menu_demo_request_categ" sequence="1"/>




</data>
</odoo>



