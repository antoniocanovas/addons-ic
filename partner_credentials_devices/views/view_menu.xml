<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_configuration_devices" model="ir.actions.act_window">
        <field name="auto_search" eval="True"/>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="multi" eval="False"/>
        <field name="name">Devices</field>
        <field name="res_model">device.types</field>
        <field name="search_view_id" eval="False"/>
        <field name="src_model" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
        <field name="view_type">form</field>
    </record>

    <record id="menu_main_configuration" model="ir.ui.menu">
        <field name="active" eval="True"/>
        <field name="name">Configuration</field>
        <field name="parent_id" ref="partner_credentials.menu_partner_credentials"/>
        <field name="sequence">11</field>
    </record>

    <record id="menu_configuration_devices" model="ir.ui.menu">
        <field name="action" ref="action_configuration_devices"/>
        <field name="active" eval="True"/>
        <field name="name">Devices</field>
        <field name="parent_id" ref="menu_main_configuration"/>
        <field name="sequence">1</field>
    </record>


</odoo>