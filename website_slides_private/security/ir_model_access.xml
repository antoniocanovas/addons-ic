<?xml version="1.0"?>
<odoo>

  <!-- Modify empty group to existing access permission: -->
  <record id="website_slides.access_slide_slide_all" model="ir.model.access">
    <field name="name">slide.slide.all</field>
    <field name="perm_read" eval="True"/>
    <field name="perm_create" eval="False"/>
    <field name="perm_write" eval="False"/>
    <field name="perm_unlink" eval="False"/>
    <field name="group_id" eval="ref('base.group_user')"/>
  </record>

</odoo>
