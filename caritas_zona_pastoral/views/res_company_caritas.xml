<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_res_company_caritas_zonas" model="ir.ui.view">
        <field name="name">Caritas Zonas</field>
        <field name="model">res.company</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[1]" position="after">
                <page name="caritas" string="Caritas">
                    <group>
                        <field name="fead"/>
                        <field name="faga"/>
                        <field name="azagra"/>
                        <field name="zona_pastoral_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>




</odoo>
