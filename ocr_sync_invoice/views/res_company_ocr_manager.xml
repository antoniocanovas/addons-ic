<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_company_ocr_delivery" model="ir.ui.view">
        <field name="name">OCR company manager</field>
        <field name="model">res.company</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='OCR']/group/field[@name='api_domain']" position="after">
                        <field name="ocr_delivery_company"/>
            </xpath>
        </field>
    </record>

</odoo>
