<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="sale_udo_target_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.target</field>
        <field name="name">Editable Tree View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="sale_udo_style_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.style</field>
        <field name="name">Editable Tree View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="sale_udo_type_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.type</field>
        <field name="name">Editable Tree View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="sale_udo_quality_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.quality</field>
        <field name="name">Editable Tree View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="sale_udo_template_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="ref"/>
                <field name="description"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.template</field>
        <field name="name">Editable Tree View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="sale_udo_template_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h2>
                        <field name="name" required="1"/>
                    </h2>
                    <group>
                        <field name="ref"/>
                        <field name="description"/>
                    </group>
                    <group>
                        <group>
                            <field name="product_brand_id"/>
                            <field name="style_id"/>
                            <field name="target_id"/>
                        </group>
                        <group>
                            <field name="type_id"/>
                            <field name="quality_id"/>
                            <field name="department_id"/>
                        </group>
                    </group>
                    <field name="line_ids">
                        <tree editable="bottom">
                            <field name="product_id" required="1"/>
                            <field name="name"/>
                            <field name="product_uom_qty"/>
                            <field name="product_uom"/>
                        </tree>
                    </field>
                </sheet>
            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.template</field>
        <field name="name">UDO Template form View</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

    <record id="sale_udo_line_action_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree create='0'>
                <field name="product_id"/>
                <field name="product_uom_qty"/>
                <field name="product_uom"/>
                <field name="price_unit"/>
                <field name="subtotal"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">udo.line</field>
        <field name="name">UdO line View </field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>
</odoo>
