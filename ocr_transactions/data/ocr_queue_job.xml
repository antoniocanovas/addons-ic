<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <data noupdate="1">

        <record id="ir_cron_ocr_queue_jobs" model="ir.cron">
            <field name="name">=> OCR transactions queue</field>
            <field ref="model_res_company" name="model_id" />
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">2</field>
             <field name="channel_id">root</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
             <field name="run_as_queue_job">True</field>
            <field eval="False" name="doall" />
            <field name="state">code</field>
            <field name="code">model.prepare_ocr_transactions()</field>
        </record>
    </data>

</odoo>