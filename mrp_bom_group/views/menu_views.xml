<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="action_mrp_bom_group" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">Groups</field>
        <field name="res_model">mrp.bom.group</field>
        <!--<field name="search_view_id" eval="isets_tree_search"/>-->
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="menu_setting_bom_group" model="ir.ui.menu">
        <field name="action" ref="action_mrp_bom_group"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('mrp.group_mrp_manager')])]"/>
        <field name="name">Group</field>
        <field name="parent_id" ref="mrp.menu_mrp_configuration"/>
        <field name="sequence">10</field>
        <field name="web_icon" eval="False"/>
    </record>


</odoo>
