<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="iset_report" model="ir.actions.report">
            <field name="name">iSet</field>
            <field name="model">isets</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">isets.iset_report_document</field>
            <field name="report_file">isets.iset_report_document</field>
            <field name="print_report_name">((str(object.type_id.name) or ''))</field>
            <field name="attachment">((str(object.type_id.name) or '')+'.pdf')</field>
            <field name="binding_model_id" ref="model_isets"/>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>
