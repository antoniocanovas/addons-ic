<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="isets_timesheet_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree name="iSet_timesheet_tree" string="iSets">
                <field name="date"/>
                <field name="employee_id"/>
                <field name="project_time" sum="Projects"/>
                <field name="repair_time" sum="Repairs"/>
                <field name="mrp_time" sum="Productions"/>
                <field name="time" sum="Total"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">iset.timesheet</field>
        <field name="name">iSets TimesheetTree View</field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="isets_timesheet_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1>
                        <field name="name"/>
                    </h1>
                    <h3>Projects:</h3>
                    <field name="analytic_line_ids"/>
                    <h3>Repairs:</h3>
                    <field name="repair_fee_ids">
                        <tree>
                            <field name="repair_id"/>
                            <field name="product_id"/>
                            <field name="product_uom_qty"/>
                        </tree>
                    </field>
                    <h3>MRP:</h3>
                    <field name="mrp_productivity_ids"/>
                </sheet>
            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">iset.timesheet</field>
        <field name="name">iSets timesheet form view</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

</odoo>
