<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <data>

        <record id="ir_cron_n43_get_files" model="ir.cron">
            <field name="name">=> N43 GET FILES</field>
            <field ref="model_account_bank_statement_tesoralia" name="model_id" />
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="channel_id">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field name="state">code</field>
            <field name="code">model.automated_ftp_get_n43_files()</field>
        </record>

        <record id="ir_cron_n43_import_files" model="ir.cron">
            <field name="name">=> N43 IMPORT FILES</field>
            <field ref="model_account_bank_statement_tesoralia" name="model_id" />
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="channel_id">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field name="state">code</field>
            <field name="code">model.automated_import_files()</field>
        </record>

        <record id="ir_n43_import_files" model="ir.cron">
            <field name="name">=> N43 Importar</field>
            <field ref="model_account_bank_statement_tesoralia" name="model_id" />
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="binding_model_id" ref="model_account_bank_statement_tesoralia"/>
            <field name="state">code</field>
            <field name="code">model.import_files()</field>
        </record>

    </data>
</odoo>