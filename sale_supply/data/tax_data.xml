<?xml version="1.0" encoding="utf-8"?>
<odoo>


  <record id="supply_sale_tax" model="account.tax">
    <field name="name">0% suplidos</field>
    <field name="model_id" ref="account.model_account_tax"/>
    <field name="active" eval="True"/>
    <field name="type_tax_use">sale</field>
    <field name="amount_type">percent</field>
    <field name="amount">0</field>
    <!--<field name="tax_group_id">0</field>-->
  </record>
  <record id="supply_purchase_tax" model="account.tax">
    <field name="name">0% suplidos</field>
    <field name="model_id" ref="account.model_account_tax"/>
    <field name="active" eval="True"/>
    <field name="type_tax_use">purchase</field>
    <field name="amount_type">percent</field>
    <field name="amount">0</field>
    <!--<field name="tax_group_id">0</field>-->
  </record>

</odoo>