<?xml version='1.0' encoding='UTF-8'?>

<odoo>
    <data>

        <template id="case_docs.docs_report_document_layout">
            <div class="header">

            </div>
            <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
               <!-- <t t-call="web.address_layout"/>-->
                <t t-raw="0"/>
            </div>
            <div class="footer o_standard_footer">
                <div class="text-center" style="border-top: 1px solid black;">
                    <div t-if="report_type == 'pdf'" class="text-muted">
                        Page: <span class="page"/> / <span class="topage"/>
                    </div>
                </div>
            </div>
        </template>

        <template id="case_docs.docs_report_document_report">
            <t t-call="case_docs.docs_report_document_layout">
                <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
               <!-- <t t-call="web.address_layout"/>-->
                <t t-raw="0"/>
            </div>
                <t t-set="o"/>
                <div class="page">
                    <div class="oe_structure"/>
                    Hola
                </div>
            </t>
        </template>

        <template id="docs_report_document">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="case_docs.docs_report_document_report" t-lang="lang"/>
                    <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
               <!-- <t t-call="web.address_layout"/>-->
                <t t-raw="0"/>
            </div>
                </t>
            </t>
        </template>

    </data>
</odoo>