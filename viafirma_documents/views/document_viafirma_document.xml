<odoo>

    <record id="view_document_dms_viafirma" model="ir.ui.view">
        <field name="name">Viafirma Documents Dms</field>
        <field name="model">ir.attachment</field>
        <field name="type">tree</field>
        <field name="inherit_id" ref="documents_dms.document_dms_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="viafirma_state"/>
            </xpath>
        </field>
    </record>

</odoo>