<?xml version='1.0' encoding='UTF-8'?>
<odoo>

  <record id="action_ocr2invoice" model="ir.actions.act_window">
    <field name="auto_search" eval="True"/>
    <field name="binding_model_id" eval="False"/>
    <field name="binding_type">action</field>
    <!--<field name="context" eval="False"></field>-->
    <!-- <field name="domain" eval="[('tag_ids.facet_id.name', '=', 'OCR')]"/>-->
    <field name="filter" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="limit">80</field>
    <field name="multi" eval="False"/>
    <field name="name">OCR</field>
    <field name="res_model">ocr.invoice</field>
    <field name="search_view_id" eval="False"/>
    <field name="src_model" eval="False"/>
    <field name="target">current</field>
    <field name="type">ir.actions.act_window</field>
    <field name="usage" eval="False"/>
    <field name="view_id" eval="False"/>
    <field name="view_mode">tree,form</field>
    <field name="view_type">form</field>
  </record>

  <menuitem
          action="action_ocr2invoice"
          id="menu_ocr2invoice"
          name="OCR"
          sequence="10"
          web_icon="attachment_ocr2invoice,static/description/docs.png"
  />
  <record id="menu_attachment_ocr2invoice" model="ir.ui.menu">
    <field name="action" ref="action_ocr2invoice"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">OCR</field>
    <field name="parent_id" ref="menu_ocr2invoice"/>
    <field name="sequence">1</field>
    <field name="web_icon" eval="False"/>
  </record>



</odoo>