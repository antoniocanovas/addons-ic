<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="suasor_invoice_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form create="false" edit="false" delete="false">
                <sheet string="Suasor">
                    <h2>
                        <field name="name"/>
                    </h2>
                    <group>
                        <field name="invoice_id"/>

                    </group>
                </sheet>

            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">suasor.invoice</field>
        <field name="name">Suasor Invoice Form</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

    <record id="suasor_invoice_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree create="false" delete="false" edit="false">
                <field name="name"/>
                <field name="create_date"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">suasor.invoice</field>
        <field name="name">Suasor invoice Tree</field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

    <record id="action_suasor_invoice" model="ir.actions.act_window">
        <field name="auto_search" eval="True"/>
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="context">{}</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="multi" eval="False"/>
        <field name="name">Suasor</field>
        <field name="res_model">suasor.invoice</field>
        <field name="search_view_id" eval="False"/>
        <field name="src_model" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
        <field name="view_type">form</field>
    </record>

    <menuitem
            action="action_suasor_invoice"
            id="menu_suasor"
            name="Suasor"
            sequence="10"
            web_icon=""
    />


</odoo>
