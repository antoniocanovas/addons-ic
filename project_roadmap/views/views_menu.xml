<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <menuitem
            action="action_view_project_roadmap"
            id="roadmap_main"
            name="Roadmap"
            sequence="3"
            web_icon="roadmap,static/description/icon.png"
    />

  <record id="menu_project_roadmap_tree" model="ir.ui.menu">
    <field name="action" ref="action_view_project_roadmap"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">Roadmap</field>
    <field name="parent_id" ref="roadmap_main"/>
    <field name="sequence">3</field>
    <field name="web_icon" eval="False"/>
  </record>

  <record id="menu_html_tree_projects" model="ir.ui.menu">
    <field name="action" ref="action_html_tree_template"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">Roadmap resume</field>
    <field name="parent_id" ref="project.menu_project_config"/>
    <field name="sequence">5</field>
    <field name="web_icon" eval="False"/>
  </record>

</odoo>